{
  "name": "@xwat/gl-matrix",
  "version": "0.2.2",
  "description": "@xwat/gl-matrix",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json",
    "dev": "tsc -p ./tsconfig.build.json -w",
    "bench": "vitest bench",
    "build:wasm": "esbuild --bundle --platform=node ./build/index.tsx | node && tsc -p ./tsconfig.wasm.json",
    "test": "vitest"
  },
  "exports": {
    ".": "./dist/index.js",
    "./wasm/*": "./dist/wasm/*.js"
  },
  "typesVersions": {
    "*": {
      ".": ["./dist/index.d.ts"],
      "./wasm/*": ["./dist/wasm/*.d.ts"]
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {},
  "dependencies": {
    "@xwat/build": "workspace:*",
    "@xwat/wabt": "workspace:*",
    "@xwat/xwat": "workspace:*",
    "gl-matrix": "^3.4.3",
    "gl-matrix-wasm": "^0.8.0"
  }
}

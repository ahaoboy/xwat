{
  "name": "@xwat/feature-detect",
  "version": "0.2.2",
  "description": "@xwat/feature-detect",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json",
    "build-tsx": "tsx --help",
    "build:wasm": "tsx ./build/index.tsx && tsc -p ./tsconfig.wasm.json",
    "dev": "tsc -p ./tsconfig.build.json -w",
    "test": "vitest",
    "bench": "vitest bench"
  },
  "exports": {
    ".": "./dist/index.js",
    "./wasm/*": "./dist/wasm/*.js"
  },
  "typesVersions": {
    "*": {
      ".": ["./dist/index.d.ts"],
      "./wasm/*": ["./wasm/*.d.ts"]
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
  },
  "dependencies": {
    "@xwat/build": "workspace:*",
    "@xwat/wabt": "workspace:*",
    "@xwat/xwat": "workspace:*"
  }
}
